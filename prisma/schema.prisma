generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model admins {
  cn String @id @db.VarChar(10)
}

model bookings {
  datetime DateTime @db.Timestamptz(6)
  room     String   @db.VarChar(5)
  cn       String   @db.VarChar(10)

  @@id([datetime, room])
}
